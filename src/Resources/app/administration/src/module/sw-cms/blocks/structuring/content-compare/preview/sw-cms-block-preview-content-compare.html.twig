{% block sw_cms_block_preview_content_compare %}
    <div class="sw-cms-block-preview-content-compare">
        <div class="sw-cms-block-preview-content-compare__content">
            <div class="sw-cms-block-preview__empty-stage">
                <sw-icon name="default-basic-plus-circle" size="18" />
                <p>{{ $tc('sw-cms.detail.label.addBlocks') }}</p>
            </div>

            <div class="sw-cms-block-preview-content-compare__devider">
                <div class="sw-cms-block-preview-content-compare__devider-button">
                    <sw-icon name="default-arrow-move-vertical-axis" size="16" />
                </div>
            </div>
        </div>
    </div>
{% endblock %}
